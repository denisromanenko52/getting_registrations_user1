@model List<string>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Email Table</title>
    <style>
        table {
            border-collapse: collapse;
            width: 50%;
            margin: 20px auto;
        }

        th, td {
            border: 1px solid black;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>Email Table</h1>
    <table>
        <tr>
            <th>Email</th>
        </tr>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item</td>
            </tr>
        }
    </table>
</body>
</html>
